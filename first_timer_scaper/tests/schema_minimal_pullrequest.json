{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "format" : "minimal",
  "properties": {
    "full_name": {
      "type": "string",
      "pattern": "^[^/#]+/[^/#]+#[0123456789]+$"
    },
    "number": {
      "format": "positiveInteger"
    },
    "name": {
      "type": "string",
      "pattern": "^[^/#]+#[0123456789]+$"
    }
  },
  "required": ["full_name", "number", "name"]
}
